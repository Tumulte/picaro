{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "useUnknownInCatchVariables": true,
    "declaration": true,
    "baseUrl": ".",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "paths": {
      "@/*": [
        "./Picaro/Front/*"
      ],
      "@types": [
        "./Picaro/Front/src/types"
      ],
      "@utils": [
        "./Picaro/utils"
      ],
      "@libs/*": [
        "./Picaro/Libs"
      ],
      "@data/*": [
        "./Picaro/Data"
      ]
    },
    "exclude": [
      "node_modules"
    ]
  },
  "resolveJsonModule": true
}
