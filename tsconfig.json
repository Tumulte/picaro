{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "paths": {
      "@/*": [
        "./Picaro/Front/*"
      ],
      "@types": [
        "./Picaro/Types"
      ],
      "@libs/*": [
        "./Picaro/Libs"
      ]
    },
    "exclude": [
      "node_modules"
    ]
  }
}
