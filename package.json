{
	"name": "javascript-development-environment",
	"version": "0.0.1",
	"repository": {
		"url": "https: //github.com/Tumulte/nodeDevEnv.git",
		"email": "info@e-rouge.net"
	},
	"description": "Initial Json for a Node project",
	"scripts": {
		"prestart": "node app/startMessage.js",
		"start": "node ./App/server.js",
		"init": "node ./App/initiateApp.js",
		"start:dev": "npm-run-all --parallel open:src security-check test:watch dev-server",
		"open:src": "set NODE_ENV='development' && nodemon --inspect ./App/server.js  --watch App/**/*  --ignore App/Static/*",
		"security-check": "npm audit",
		"test": "jest --coverage",
		"test:watch": "npm run test -- --watch",
		"build": "webpack --mode=development --config webpack.config.dev.js",
		"dev-server": "webpack-dev-server --port 3000"
	},
