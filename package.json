  },
  "description": "Initial Json for a Node project",
  "bin": {
    "rfcli": "./App/Cli/create-site/bin/cli.js"
  },
  "scripts": {
    "start": "node ./App/Dist/server.js",
    "start:dev": "npm-run-all --parallel start:front start:back",
    "start:back": "export NODE_ENV='development' set NODE_ENV='development' && nodemon --exec babel-node ./App/Src/server.js  --watch App/  --ignore App/Static/ App/Dist/",
    "start:front": "webpack serve --config webpack.config.dev.js --progress",
    "test": "node App/Tests/testCron.js",
    "jest": "jest --coverage",
    "test:int": "cypress run",
    "test:watch": "jest --watch",
    "build": "npm-run-all --parallel build:front build:back",
    "build:front": "webpack --mode=production --config webpack.config.prod.js",
    "build:back": "babel ./App/Src -d ./App/Dist",
    "build:dev": "webpack --mode=development --config webpack.config.dev.js"
  },
  "keywords": [
    "cli",
    "rfcli"
  ],
