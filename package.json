  "scripts": {
    "start": "node ./App/server.js",
    "init": "node ./App/initiateApp.js",
    "start:dev": "npm-run-all --parallel open:src security-check test",
    "open:src": "set NODE_ENV='development' && nodemon --exec babel-node ./App/server.js  --watch App/**/*.js  --ignore App/Static/*",
    "security-check": "npm audit",
    "test": "node App/Tests/testCron.js",
    "jest": "jest --coverage",
    "test:int": "cypress run",
    "test:watch": "npm run test -- --watch",
    "build": "webpack --mode=production --config webpack.config.prod.js",
    "build:dev": "webpack --mode=development --config webpack.config.dev.js"
  },
