  "jest": {
    "testResultsProcessor": "./node_modules/jest-stare",
    "coverageDirectory": "App/Tests/coverage",
    "verbose": false,
    "watchPathIgnorePatterns": [
      "<rootDir>/App/Dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/App/Tests"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@store/(.*)$": "<rootDir>/App/Store/$1",
      "^@components/(.*)$": "<rootDir>/App/Ui/Components/$1",
      "~/(.*)": "<rootDir>/$1"
    },
    "transform": {
      ".*\\.(js)$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    }
  },
  "jestJsonReporter": {
    "outputFile": "App/jestResults.json"
  },
  "jest-stare": {
    "resultDir": "App/Tests"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  }
}
