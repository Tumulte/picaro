{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "skipLibCheck": true,
    "paths": {
      "@components/*": [
        "../Front/src/components/*"
      ],
      "@utils/*": [
        "../Front/utils/*"
      ],
      "@stores/*": [
        "../Front/stores/*"
      ],
      "@libs/*": [
        "./../Libs/*"
      ],
      "@types": [
        "./../Types"
      ],
      "@data": [
        "./../Data"
      ]
    }
  },
  "types": [
    "cypress",
    "node"
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "../Front/src/**/*.ts",
    "../Front/src/**/*.d.ts",
    "../Front/src/**/*.tsx",
    "../Front/src/**/*.vue"
  ]
}
